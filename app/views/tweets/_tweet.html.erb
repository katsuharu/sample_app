<tr>
	<td class="tweet_info">
		<%= User.find_by(id: tweet.user_id).name %>
	</td>

	<td class="tweet_cnt">
		<%= tweet.content %>
	</td>
	<td class="tweet_info">
		<%= Category.find_by(id: tweet.category_id).name %>
	</td>
	
	<td class="tweet_info">
		<%= link_to "+", tweet_path(tweet.id), remote: true %>
	</td>
</tr>